{
  "expo": {
    "name": "Coppel PTL",
    "slug": "coppel-ptl",
    "owner": "coppelptl",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Logo_apk.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/coppel_imagen_carga.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffcc00"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Esta app necesita acceso a la cámara para escanear códigos de barras."
      }
    },
    "android": {
      "package": "com.anonymous.coppelptl",
      "versionCode": 1,
      "label": "Coppel PTL",
      "icon": "./assets/Logo_apk.png",
      "permissions": [
        "android.permission.CAMERA"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/Logo_apk.png",
        "backgroundColor": "#ffcc00"
      }
    },
    "web": {
      "favicon": "./assets/coppel_imagen_carga.png"
    },
    "plugins": [
      "expo-barcode-scanner"
    ],
    "extra": {
      "eas": {
        "projectId": "f8e1dca2-554f-4214-8c3e-249a82b84f20"
      }
    },
    "jsEngine": "hermes"
  }
}
